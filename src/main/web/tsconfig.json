{
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"moduleResolution": "node",
		"sourceMap": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"sourceRoot": "/app-src",
		"outDir": "dist",
		"rootDir": "src",
		"removeComments": false,
		"noImplicitAny": false
	},
	"exclude" : [
		"node_modules"
	]
}